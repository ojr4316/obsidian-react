{
    "format_version": "1.8.0",
    "minecraft:client_entity": {
        "description": {
            "identifier": "${projectName}:${name}",
            "materials": {
                "default": "generic_entity"
            },
            "textures": {
                "default": "textures/entity/${name}"
            },
            "geometry": {
                "default": "geometry.${name}"
            },
            "render_controllers": [
                "controller.render.agent"
            ],
            "spawn_egg": {
                "base_color": "#FFFFFF",
                "overlay_color": "#FFFFFF"
            }
        }
    }
}